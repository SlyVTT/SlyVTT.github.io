{
\pard\plain \s17\sa60\sb30\widctlpar\qj \fs22\cgrid {\s17\sa60\sb30\widctlpar\qj \fs22\cgrid 
Example of program using {\b CommuterWidget} controls\par
The following simple example demonstrate how to use the {\b CommuterWidget} controls.  \par
{\bkmkstart AAAAAAAACZ}
{\bkmkend AAAAAAAACZ}
{{\pard\plain \s4\sb240\sa60\keepn\widctlpar\adjustright \b\f1\fs20\cgrid 
Examples of usage:
\par}
{\tc\tcl4 \v Examples of usage:}
{
\par
\pard\plain \s40\li0\widctlpar\adjustright \shading1000\cbpat8 \f2\fs16\cgrid {\cf21 #include "Toolkit/GUIToolkit.h"}\par
\par
{\cf18 int} main ( {\cf18 int} argc, {\cf18 char}** argv )\par
\{\par
    {\cf18 bool} done = {\cf17 false};\par
\par
    WidgetApplication *MyApp = {\cf17 new} WidgetApplication();\par
    MyApp->setuniformbackgroundcolor(0, 0, 0);\par
\par
    DesktopWidget *desktop1 = {\cf17 new} DesktopWidget( ({\cf18 char}*) {\cf22 "First Desktop"}, 0, 0, SCREEN_WIDTH, SCREEN_HEIGHT, {\cf17 nullptr} );\par
\par
   WindowWidget *window = {\cf17 new} WindowWidget( ({\cf18 char}*) {\cf22 "Examples of CommuterWidgets"}, 5, 5, 310, 230, desktop1 );\par
   ContainerVWidget *containervert = {\cf17 new} ContainerVWidget( ({\cf18 char}*) {\cf22 "container"}, 1, 1, 1, 1, window );\par
\par
   LabelWidget *labelB = {\cf17 new} LabelWidget( ({\cf18 char}*) {\cf22 "Examples of CommuterWidget"}, 1, 1, 1, 1, containervert );\par
\par
   ContainerHWidget *contH1 = {\cf17 new} ContainerHWidget( ({\cf18 char}*) {\cf22 "container"}, 1, 1, 1, 1, containervert );\par
   CommuterWidget *com11 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.1 Normal"}, 1, 1, 1, 1, contH1 );\par
   com11->settype( CommuterWidget::style1 );\par
   com11->setnormal();\par
   CommuterWidget *com12 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.1 Reverse"}, 1, 1, 1, 1, contH1 );\par
   com12->settype( CommuterWidget::style1 );\par
   com12->setreversed();\par
\par
   ContainerHWidget *contH2 = {\cf17 new} ContainerHWidget( ({\cf18 char}*) {\cf22 "container"}, 1, 1, 1, 1, containervert );\par
   CommuterWidget *com21 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.2 Normal"}, 1, 1, 1, 1, contH2 );\par
   com21->settype( CommuterWidget::style2 );\par
   com21->setnormal();\par
   CommuterWidget *com22 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.2 Reverse"}, 1, 1, 1, 1, contH2 );\par
   com22->settype( CommuterWidget::style2 );\par
   com22->setreversed();\par
\par
   ContainerHWidget *contH3 = {\cf17 new} ContainerHWidget( ({\cf18 char}*) {\cf22 "container"}, 1, 1, 1, 1, containervert );\par
   CommuterWidget *com31 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.3 Normal"}, 1, 1, 1, 1, contH3 );\par
   com31->settype( CommuterWidget::style3 );\par
   com31->setnormal();\par
   CommuterWidget *com32 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.3 Reverse"}, 1, 1, 1, 1, contH3 );\par
   com32->settype( CommuterWidget::style3 );\par
   com32->setreversed();\par
\par
   ContainerHWidget *contH4 = {\cf17 new} ContainerHWidget( ({\cf18 char}*) {\cf22 "container"}, 1, 1, 1, 1, containervert );\par
   CommuterWidget *com41 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.4 Normal"}, 1, 1, 1, 1, contH4 );\par
   com41->settype( CommuterWidget::style4 );\par
   com41->setnormal();\par
   CommuterWidget *com42 = {\cf17 new} CommuterWidget( ({\cf18 char}*) {\cf22 "St.4 Reverse"}, 1, 1, 1, 1, contH4 );\par
   com42->settype( CommuterWidget::style4 );\par
   com42->setreversed();\par
\par
    {\cf20 // the button to be clicked to quit}\par
    ButtonWidget *buttonQuit = {\cf17 new} ButtonWidget( ({\cf18 char}*) {\cf22 "Quit [CTRL]+[ESC]"}, 1, 1, 1, 1, contH );\par
\par
    {\cf20 // We ask for an automatic positioning of the widgets in the grid}\par
    {\cf20 // Note : this will fully override the dimensions given to the Widgets manually}\par
    window->adjust();\par
\par
    MyApp->addchild( desktop1 );\par
\par
    KeyboardTask *keyboard = MyApp->getkeyboardhandler();\par
\par
    {\cf20 // We render the app for the first time so what we can see what's happening on the screen}\par
    MyApp->render();\par
\par
    {\cf19 while} (!done)\par
    \{\par
\par
        MyApp->logic();\par
\par
        {\cf19 if} ((keyboard->kbCTRL && keyboard->kbESC) || buttonQuit->ispressed()) done = {\cf17 true};\par
\par
    \}\par
\par
    {\cf19 return} 0;\par
\}\par
